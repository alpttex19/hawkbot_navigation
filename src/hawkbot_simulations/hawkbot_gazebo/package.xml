<?xml version="1.0"?>
<package format="2">
  <name>hawkbot_gazebo</name>
  <version>1.3.2</version>
  <description>
    Gazebo simulation package for the hawkbot
  </description>
  <license>Apache 2.0</license>
  <author email="hawkbot@todo.com">hawkbot</author>
  <maintainer email="hawkbot@todo.com">hawkbot</maintainer>
  <buildtool_depend>catkin</buildtool_depend>
  <depend>roscpp</depend>
  <depend>std_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>tf</depend>
  <depend>gazebo_ros</depend>
  <exec_depend>gazebo</exec_depend>
  <exec_depend>hawkbot_description</exec_depend>
  <export>
    <gazebo_ros gazebo_media_path="${prefix}"/>
    <gazebo_ros gazebo_model_path="${prefix}/models"/>
  </export>
</package>
